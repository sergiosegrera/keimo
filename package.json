{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "boilerplate": "./start.sh",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome lint",
    "shadcn": "pnpm dlx shadcn@latest add",
    "inngest-start": "pnpm dlx inngest-cli@latest dev",
    "db-push": "drizzle-kit push",
